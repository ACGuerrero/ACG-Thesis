      ",", "\n", 
      RowBox[{"Flatten", "[", 
       RowBox[{"{", 
        RowBox[{"set", ",", " ", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"set", "[", 
            RowBox[{"[", "i", "]"}], "]"}], ",", " ", 
